{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType":"module",
    "ecmaFeatures": {
        "jsx": true
      }
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "off",
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "max-len": ["error", { "code": 100 }]
  }
}
